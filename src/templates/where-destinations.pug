        include includes/header-inner
        section.search.search--inner
            div.search__breadcrumbs
                a.link(href="#")
                    span Главная
                span.slash /
                span.current Такси в аэропорт Симферополя
            h1 Такси-трансфер в аэропорт Симферополя
            div.container
                div.row
                    form.col-md-10.offset-1
                        div.row
                            div.col-md-4
                                fieldset
                                    input.input(id="from")
                                    label(for="from") Откуда

                            div.col-md-4
                                fieldset
                                    input.input.focus(id="where" value="Симферополь")
                                    label(for="where") Куда

                            div.col-md-2
                                fieldset
                                    input.input(id="passengers")
                                    label(for="passengers") Пассажиры

                            div.col-md-2
                                button Найти



